{
	"extends": "tslint:recommended",
	"rules": {
		"indent": [true, "tabs", 4],
		"array-type": false,
		"arrow-parens": false,
		"import-blacklist": [true, "rxjs/Rx"],
		"interface-name": false,
		"max-classes-per-file": false,
		"max-line-length": [true, 140],
		"no-console": [false],
		"no-mixed-spaces-and-tabs": ["warn"],
		"no-unused-expression": [false]
	}
}
